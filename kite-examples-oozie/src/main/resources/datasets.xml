<datasets>
  <dataset name="Person" frequency="${coord:hours(1)}" initial-instance="2014-04-24T00:00Z" timezone="UTC">
    <uri-template>${datasetRepositoryUri}?dataset-name=Person&amp;partition-key=[${YEAR}${MONTH}${DAY}${HOUR}${MINUTE}]
    </uri-template>
  </dataset>
  <dataset name="PersonOutcomes" frequency="${coord:hours(1)}" initial-instance="2014-04-24T00:00Z" timezone="UTC">
    <uri-template>${datasetRepositoryUri}?dataset-name=PersonOutcomes&amp;partition-key=[${YEAR}${MONTH}${DAY}${HOUR}${MINUTE}]
    </uri-template>
  </dataset>
  <dataset name="PersonSummary" frequency="${coord:hours(1)}" initial-instance="2014-04-24T00:00Z" timezone="UTC">
    <uri-template>${datasetRepositoryUri}?dataset-name=PersonSummary&amp;partition-key=[${YEAR}${MONTH}${DAY}${HOUR}${MINUTE}]
    </uri-template>
  </dataset>
</datasets>